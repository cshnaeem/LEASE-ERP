                            @(Html.Kendo()
                        .TextBox()
                        .Name("Mobile1")
                        .Enable(false)
                        .HtmlAttributes(new { @style = "width:100%" })
                        )
                        </div>
                        <div class="col-md-4 col-sm-4">
                            @Html.Label("Mobile2")
                            @(Html.Kendo()
                        .TextBox()
                        .Name("Mobile2")
                        .Enable(false)
                        .HtmlAttributes(new { @style = "width:100%" })
                        )
                        </div>
                        <div class="col-md-4 col-sm-4">
                            @Html.Label("NIC")
                            @(Html.Kendo()
                        .TextBox()
                        .Name("NICStatus")
                        .Enable(false)
                        .HtmlAttributes(new { @style = "width:100%" })
                        )
                        </div>

                        <div class="col-md-4 col-sm-4">
                            @Html.Label("NoOfCheques")
                            @(Html.Kendo()
                        .TextBox()
                        .Name("NoOfCheques")
                        .Enable(false)
                        .HtmlAttributes(new { @style = "width:100%" })
                        )
                        </div>
                        <div class="col-md-4 col-sm-4">
                            @Html.Label("NoOfGuarantors")
                            @(Html.Kendo()
                        .TextBox()
                        .Name("NoOfGuarantors")
                        .Enable(false)
                        .HtmlAttributes(new { @style = "width:100%" })
                        )
                        </div>
                    </div>
                    <div class="row">
                        <h4 class="card-title">Audit</h4>
                        @using (Ajax.BeginForm("Audit", null, new AjaxOptions { HttpMethod = "POST", OnSuccess = "OnSave" }, new { @Id = "frm" }))
                        {
                            <div class="row">
                                @Html.Hidden("AccNo", 0)
                                <div class="col-md-3 col-sm-4">
                                    @(Html.Kendo()
                            .CheckBox()
                            .Label("Mobile")
                            .Name("Mobile")
                            .Checked(false)
                            .HtmlAttributes(new { @onchange = "CalcCompletion()" })
                                )
                                </div>
                                <div class="col-md-3 col-sm-4">
                                    @(Html.Kendo()
                            .CheckBox()
                            .Label("NIC")
                            .Name("NIC")
                            .Checked(false)
                            .HtmlAttributes(new {  @onchange = "CalcCompletion()" })
                                )
                                </div>
                                <div class="col-md-3 col-sm-4">
                                    @(Html.Kendo()
                            .CheckBox()
                            .Label("Affidavit")

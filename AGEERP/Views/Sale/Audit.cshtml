                                )
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-3 col-sm-4">
                                    @(Html.Kendo()
                            .CheckBox()
                            .Label("Guarantor1")
                            .Name("Guarantor1")
                            .Checked(false)
                            .HtmlAttributes(new {  @onchange = "CalcCompletion()" })
                                )
                                </div>
                                <div class="col-md-3 col-sm-4">
                                    @(Html.Kendo()
                            .CheckBox()
                            .Label("Guarantor2")
                            .Name("Guarantor2")
                            .Checked(false)
                            .HtmlAttributes(new {  @onchange = "CalcCompletion()" })
                                )
                                </div>


                                <div class="col-md-3 col-sm-4">
                                    @(Html.Kendo()
                            .CheckBox()
                            .Label("Thumb")
                            .Name("Thumb")
                            .Checked(false)
                            .HtmlAttributes(new {  @onchange = "CalcCompletion()" })
                                )
                                </div>
                                <div class="col-md-3 col-sm-4">
                                    @(Html.Kendo()
                            .CheckBox()
                            .Label("Pic")
                            .Name("Pic")
                            .Checked(false)
                            .HtmlAttributes(new {  @onchange = "CalcCompletion()" })
                                )
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-3 col-sm-4">
                                    @(Html.Kendo()

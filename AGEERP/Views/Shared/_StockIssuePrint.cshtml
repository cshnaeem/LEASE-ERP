@model IEnumerable<AGEERP.CrReports.IssueRVM>
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet">

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/1.3.2/jspdf.min.js"></script>


    <style type="text/css">

        body {
            padding-top: 0px;
            background: #f1f4f7;
            color: #000;
        }


        @@media print {
            #bkpos_wrp {
                display: none;
            }

            .page-break {
                display: block;
                page-break-before: always;
            }
        }

        td, th {
            padding: 0px;
        }
    </style>
</head>

<body>
    <div class="row" id="bkpos_wrp" style="padding-bottom: 10px; padding-top: 10px;">

    </div>
    <center>
        @foreach (var item in Model)
        {
            <div class="page-break" style="margin-top: 5px;">
                <table border="0" style="border-collapse: collapse; margin-bottom: 20px;" width="140px" height="auto">
                    <tr>
                        <td style="font-family: Arial, Helvetica, sans-serif; text-align: center; font-size: 12px;">
                        </td>
                    </tr>
                    <tr>
                        <td style="font-family: Arial, Helvetica, sans-serif; text-align: center; font-size: 12px;">
                        </td>
                    </tr>
                    <tr>
                        <td style="font-family: Arial, Helvetica, sans-serif; text-align: center; font-size: 11px;">
                        </td>
                    </tr>
                    <tr>
                        <td style="font-family: Arial, Helvetica, sans-serif; text-align: center; font-size: 11px;">
                            @item.LocCode
                        </td>
                    </tr>
                </table>

            </div>
            <!--ADD_PAGE-->
        }


    </center>
    <div class="row" id="bkpos_wrp">
        <div class="col-md-12">
            <center></center>
        </div>
    </div><!-- /.row -->
    <script>
   

         $(document).ready(function () {
             var pdf = new jsPDF('p', 'pt', 'letter');
  pdf.canvas.height = 72 * 11;
  pdf.canvas.width = 72 * 8.5;
         var htm = document.getElementsByTagName("html")[0];
  pdf.fromHTML(htm);

  pdf.save('test.pdf');
        });
    </script>
</body>

</html>





